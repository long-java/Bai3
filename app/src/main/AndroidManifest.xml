<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thecoffeehouse">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TheCoffeeHouse">
        <activity android:name=".DetailItemOrder"></activity>
        <activity android:name=".NotificationActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".test_screen" />
        <activity android:name=".account_screen" />
        <activity android:name=".map_screen" />
        <activity android:name=".DetailItemHomeActivity" />
        <activity android:name=".orderScreen" />
        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_api_key" />
    </application>

</manifest>